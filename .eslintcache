Search} = this.state;\r\n        this.props.onSearch(inputSearch);\r\n    }\r\n    \r\n    handleSignOut = () => {\r\n        this.props.handleLogOut();\r\n    }\r\n\r\n    render() {\r\n        let {carts, favorites, login} = this.props;\r\n        let {name, avatar, isLogin} = login;\r\n        let newCarts = [];\r\n        let newFavorites = [];\r\n        let total = 0;\r\n        if(carts.length > 0)\r\n            carts = carts.map(cart => {\r\n                total += cart.price * cart.quantity;\r\n                if(newCarts.find(elm => elm.slug === cart.slug))\r\n                    return '';\r\n                else {\r\n                    newCarts.push(cart);\r\n                    return (\r\n                        <li>\r\n                            <Link to={`/products/${cart.slug}`}>\r\n                                <div className=\"cart-img\">\r\n                                    <img src={cart.mainImg} alt=\"\"/>\r\n                                </div>\r\n                                <div className=\"cart-item\">\r\n                                    <h3>\r\n                                        {cart.name.toLowerCase()}\r\n                                    </h3>\r\n                                    <p>{numeral(cart.price).format(0,0)} vnd</p>\r\n                                </div>\r\n                            </Link>\r\n                        </li>\r\n                    );\r\n                }    \r\n            });\r\n            if(favorites.length > 0) {\r\n                newFavorites = favorites.map(favorite => {\r\n                    return (\r\n                        <li>\r\n                            <Link to={`/products/${favorite.slug}`}>\r\n                                <div className=\"cart-img\">\r\n                                    <img src={favorite.mainImg} alt=\"\"/>\r\n                                </div>\r\n                                <div className=\"cart-item\">\r\n                                    <h3>\r\n                                        {favorite.name.toLowerCase()}\r\n                                    </h3>\r\n                                    <p>{numeral(favorite.price).format(0,0)} vnd</p>\r\n                                </div>\r\n                            </Link>\r\n                        </li>\r\n                    )\r\n                });\r\n            }\r\n        return (\r\n            <nav className=\"navbar navbar-expand-lg navbar-light bg-light\" >\r\n                <div className=\"container\">\r\n                    <Link to='/'>\r\n                        <img src=\"https://ananas.vn/wp-content/themes/ananas/fe-assets/images/svg/Logo_Ananas_Header.svg\" alt=\"\"/>                \r\n                    </Link>\r\n                    <button className=\"navbar-toggler\" type=\"button\" data-bs-toggle=\"collapse\" \r\n                            data-bs-target=\"#navbarSupportedContent\" aria-controls=\"navbarSupportedContent\" \r\n                            aria-expanded=\"false\" aria-label=\"Toggle navigation\">\r\n                        <span className=\"navbar-toggler-icon\"></span>\r\n                    </button>\r\n                    <div className=\"collapse navbar-collapse\" id=\"navbarSupportedContent\">\r\n                        <ul className=\"navbar-nav me-auto mb-2 mb-lg-0\">\r\n                            <li className=\"nav-item\">\r\n                                <Link to='/' className=\"nav-link active\" aria-current=\"page\">home</Link>\r\n                            </li>\r\n                            <li className=\"nav-item\">\r\n                                <Link to='/products' className=\"nav-link\" aria-current=\"page\">all shoes</Link>\r\n                            </li>\r\n                            <li className=\"nav-item\">\r\n                                <Link to='/products/men' className=\"nav-link\" aria-current=\"page\">men</Link>\r\n                            </li>\r\n                            <li className=\"nav-item\">\r\n                                <Link to='/products/women' className=\"nav-link\" aria-current=\"page\">women</Link>\r\n                            </li>\r\n                        </ul>\r\n                        <form className=\"d-flex\">\r\n                            <input className=\"form-control me-2\" type=\"search\" \r\n                                    onChange={this.handleInputSearch} \r\n                                    placeholder=\"Search\" aria-label=\"Search\" />\r\n                            <button className=\"btn\" onClick={this.handleSearch}>\r\n                                <Link to='/search'>\r\n                                    <i class=\"fas fa-search\"></i>\r\n                                </Link>\r\n                            </button>\r\n                        </form>\r\n                        {/* FAVORITE LIST */}\r\n                        <div className=\"cart\">\r\n                            <div className=\"cart-icon\">\r\n                                <span>{favorites ? favorites.length : 0}</span><i class=\"fas fa-heart\"></i>\r\n                            </div>\r\n                            <div className=\"cart-content\">\r\n                                <ul>\r\n                                   {newFavorites}\r\n                                </ul>\r\n                            </div>\r\n                        </div>\r\n                        <div className=\"cart\">\r\n                            <div className=\"cart-icon\">\r\n                                <span>{carts ? carts.length : 0}</span> <i class=\"fas fa-shopping-bag\"></i>\r\n                            </div>\r\n                            <div className=\"cart-content\">\r\n                                <ul>\r\n                                   {carts}\r\n                                   <li className='cart-total'>\r\n                                        <p>Total: {numeral(total).format(0,0)} VND</p>\r\n                                        <Link to='/cart'>Go to cart</Link>\r\n                                    </li>\r\n                                </ul>\r\n                            </div>\r\n                        </div>\r\n                        <div>\r\n                           {name ? ''\r\n                             : <GoogleLogin  \r\n                                clientId=\"586157721906-8ca913t35i5u9lk76vgcms0oi9360ff9.apps.googleusercontent.com\"\r\n                                buttonText=\"Login\"\r\n                                onSuccess={this.responseGoogle}    \r\n                                onFailure={this.responseGoogle}\r\n                                cookiePolicy={'single_host_origin'}/>\r\n                            }\r\n                        </div>\r\n                        <div className='user'>\r\n                            {avatar ? \r\n                                <>\r\n                                    <img src={avatar} alt=\"\"/>\r\n                                     {name}\r\n                                    <button className=\"btn btn-logout\" title='Sign-out'\r\n                                            onClick={()=>this.handleSignOut()}\r\n                                    >\r\n                                        <i class=\"fas fa-sign-out-alt\"></i>\r\n                                    </button>\r\n                                </> : ''}\r\n                        </div>\r\n                    </div>\r\n                </div>\r\n                <ToastContainer autoClose={2000} />\r\n            </nav>\r\n        )\r\n    }\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        carts: state.cartReducer,\r\n        favorites: state.favoriteReducer,\r\n        login: state.isLogin\r\n    }\r\n}\r\n\r\nconst mapDispatchToProps = (dispatch, props) => {\r\n    return {\r\n        onSearch: (inputSearch) => {\r\n            dispatch(actions.searchProduct(inputSearch));\r\n        },\r\n        getCarts: () => {\r\n            dispatch(actions.fetchCartsRequest());\r\n        },\r\n        getFavorite: () => {\r\n            dispatch(actions.fetchFavoriteRequest());\r\n        },\r\n        getProducts: () => {\r\n            dispatch(actions.fetchProductsRequest());\r\n        },\r\n        handleLogin: (name, avatar) => {\r\n            dispatch(actions.loginUser(name, avatar));\r\n        },\r\n        handleLogOut: () => {\r\n            dispatch(actions.logoutUser());\r\n        }\r\n    }\r\n}\r\n\r\nexport default connect(mapStateToProps, mapDispatchToProps)(Header);\r\n\r\n","C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\reducers\\addressUser.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\components\\SearchResult.js",["306"],"import React, { Component } from 'react';\r\nimport {connect} from 'react-redux';\r\nimport {Link} from 'react-router-dom';\r\nimport * as actions from '../action/index';\r\nimport Footer from './Footer';\r\nimport HeaderSlide from './HeaderSlide';\r\nimport ProductItem from './ProductItem';\r\n\r\nclass SearchResult extends Component {\r\n\r\n\r\n    render() {\r\n        let {search, products, favorite} = this.props;\r\n        console.log('a',search);\r\n        console.log(favorite);\r\n        console.log(products);\r\n        if(search !== '') {\r\n            products = products.filter(product => product.name.toLowerCase().includes(search.toLowerCase()));\r\n        }\r\n\r\n        if(products.length > 0) {\r\n            products = products.map(product => <ProductItem product={product} favorite={favorite}/>);\r\n        }\r\n        else products = <h2 className='search-empty'>Không tìm thấy kết quả !</h2>\r\n        return (\r\n            <div className=\"search-result\">\r\n                <HeaderSlide />\r\n                <div className=\"search-result__inner\">\r\n                    <div className=\"container py-5\">\r\n                        <div className=\"search-result__notify\">\r\n                            <h2>tìm thấy {products.length > 0 ? products.length : 0} kết quả cho <span>\"{search}\"</span></h2>\r\n                        </div>\r\n                        <div className=\"products__list row py-5\">\r\n                            {products}\r\n                        </div>\r\n                        <div className='search-result__back'>\r\n                            <Link to='/products' className='back-to-products'>xem tất cả sản phẩm</Link>\r\n                        </div>\r\n                    </div>\r\n                    \r\n                </div>\r\n                <Footer />\r\n            </div>\r\n        )\r\n    }\r\n}\r\n\r\nconst mapStateToProps = (state) => {\r\n    return {\r\n        products: state.products,\r\n        search: state.searchProducts,\r\n        favorite: state.favoriteReducer\r\n    }\r\n}\r\n\r\n\r\nexport default connect(mapStateToProps)(SearchResult);\r\n","C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\reducers\\favoriteReducer.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\components\\ProductsList.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\components\\Slide.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\Message.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\components\\Filter.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\components\\HeaderSlide.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\components\\ProductItem.js",["307"],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\components\\Home\\Category.js",[],"C:\\Users\\THINKPAD T460s\\OneDrive\\Desktop\\Main Project\\ANANAS\\src\\action\\index.js",[],"C:\\Users\\THINKPAD T460s\\OneDr